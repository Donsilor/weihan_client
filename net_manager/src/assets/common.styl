$borderColor = #dcdcdc

ellipsis()
  overflow hidden
  white-space nowrap
  text-overflow ellipsis

$iconfontColor = #34a6dd

.maskLayer
  position fixed
  top 0
  right 0
  bottom 0
  left 0
  background-color rgba(0,0,0,.7)
  z-index 999

i.el-icon-star-on
  color #f00
  font-size 10px
  height 100%
  vertical-align top
  line-height 28px
.tableWrap
  padding 0 25px 0 20px

  i

    //&.select
    //  margin-right 2%
    //  margin-left 2%
    //  border 1px solid $borderColor
    //  color #fff
    //  font-size 13px
    //  line-height 13px

  .iconfont
    font-size 20px
    margin 0 3px
    cursor pointer

    &:hover
      color $iconfontColor

  a
    cursor pointer

    &:hover
      color $iconfontColor

  .el-table
    th
      > .cell
        font-size 1.125rem
        text-align center
        font-weight normal

  .el-table
    td
      font-size 1rem
      text-align center

    td:last-child
      color #38a4de
      width 8%

  .el-table--enable-row-hover
    .el-table__body
      tr
        &:hover
          > td
            background-color #e2e7ec !important

  .el-checkbox__inner
    border-color $borderColor
.el-pagination__sizes
    display none!important
.window-content
  .iconfont
    color #ff0000 

    &:hover
      color #ff0000 
.tableWrap
  padding 0 25px 0 20px

  i

    //&.select
    //  margin-right 2%
    //  margin-left 2%
    //  border 1px solid $borderColor
    //  color #fff
    //  font-size 13px
    //  line-height 13px

  .iconfont
    font-size 20px
    margin 0 3px
    cursor pointer

    &:hover
      color $iconfontColor

  a
    &:hover
      color $iconfontColor

  .el-table
    th
      > .cell
        font-size 18px
        text-align center
        font-weight normal

  .el-table
    td
      font-size 16px
      text-align center

  .el-table--enable-row-hover
    .el-table__body
      tr
        &:hover
          > td
            background-color #e2e7ec !important

  .el-checkbox__inner
    border-color $borderColor
